{
    "ProgrammingLanguageIdentifier": "Java",
    "Objects": {
        "AbstractLanguageModel": {
            "type": "ChatGPTInterface",
            "args": {
                "model": "gpt-4-1106-preview",
                "temperature": 0.9
            }
        },
        "MetricCombiner": {
            "type": "MetricCombiner",
            "args": {
                "metrics": [
                    "DataClumpSizeMetric",
                    "DataClumpOccurenceMetric"
                ]
            }
        },
        "ByTypeNameKeyFilter": {
            "args": {
                "typeNameKey": [
                    "long instanceId",
                    "long runId",
                    "long stepAttemptId",
                    "java.lang.String stepId",
                    "long version",
               "java.lang.String workflowId"
                ]
            }
        },

        "DataClumpSizeMetric": {
            "type": "DataClumpSizeMetric"
        },
        "DataClumpOccurenceMetric": {
            "type": "DataClumpOccurenceMetric"
        },
        "refactorInstructionHandler": {
            "type": "SimpleInstructionHandler",
            "args": {
                "instructionPath": "chatGPT_templates/detectAndRefactor/definitionBased/fromScratch/instruction.template"
            }
        },
        "introductionHandler":{
            "type":"SystemInstructionHandler",
            "args": {
                "instructionPath": "chatGPT_templates/refactor_introduction.template"
            }
        },
        "AllFilesHandler": {},
        "SendHandler": {},
       
        "ByPathFilter":{
            "args":{
                "name":    "AwsSqsMonolithQueueFactory",
                "include":["common/queue/src/main/java/org/thingsboard/server/queue/provider/*"],
                "exclude":["*"]

                
            }
        },
        "RandomProbabilityFilter": {
            "args": {
                "probability": 0.3
            }
        
        }
    },
    "PipeLine": {
     
      "DataClumpDetection": {
            "handler": "dataClumpDoctorStepHandler",
            "args": {
                "handlers": [
                    "introductionHandler",
                    "refactorInstructionHandler",
                    "AllFilesHandler",
                    "SendHandler"
                ]
            }
        },
       
 "DataClumpFiltering":{
    "handler":"DataClumpFilterStepHandler",
    "args":{
        "filterName":"ByTypeNameKeyFilter"
    }
 },
       
       

    
        
        "Refactoring": {
            "handler": "DoNothingStepHandler",
            "args": {
                "handlers": [
                    "introductionHandler",
                    "refactorInstructionHandler",
                    "AllFilesHandler",
                    "SendHandler"
                ]
            }
        }
    }
}