{
    "ProgrammingLanguageIdentifier": "Java",
    "Objects": {
        "AbstractLanguageModel": {
            "type": "ChatGPTInterface",
            "args": {
                "model": "gpt-4-1106-preview",
                "temperature": 0.9
            }
        },
        "MetricCombiner": {
            "type": "MetricCombiner",
            "args": {
                "metrics": [
                    "DataClumpSizeMetric",
                    "DataClumpOccurenceMetric"
                ]
            }
        },

        "DataClumpSizeMetric": {
            "type": "DataClumpSizeMetric"
        },
        "DataClumpOccurenceMetric": {
            "type": "DataClumpOccurenceMetric"
        },
        "refactorInstructionHandler": {
            "type": "SimpleInstructionHandler",
            "args": {
                "instructionPath": "chatGPT_templates/detectAndRefactor/definitionBased/fromScratch/instruction.template"
            }
        },
        "introductionHandler":{
            "type":"SystemInstructionHandler",
            "args": {
                "instructionPath": "chatGPT_templates/refactor_introduction.template"
            }
        },
        "AllFilesHandler": {},
        "SendHandler": {},
        "LanguageModelTemplateResolver": {
            "args": {
                "${programming_language}": "Java",
                "%{examples}": "chatGPT_templates/DataClumpExamples.java",
                "%{output_format}": "chatGPT_templates/json_output_format.json",
                "%{refactor_instruction}": "chatGPT_templates/refactor_one_data_clump.template",
                "%{detected_data_clumps}": "chatGPT_templates/refactor/detected_data_clumps_minified.json",
                "%{output_format_refactor}": "chatGPT_templates/json_format_refactor_piecewise.json"
            }
        },
        "ByPathFilter":{
            "args":{
                "name":    "AwsSqsMonolithQueueFactory",
                "include":["common/queue/src/main/java/org/thingsboard/server/queue/provider/*"],
                "exclude":["*"]

                
            }
        },
        "RandomRanker": {
            "type": "RandomRanker"
        }
    },
    "PipeLine": {
        "CodeObtaining": {
            "handler": "SimpleCodeObtainingStepHandler",
            "args": {
                "path": null,
                "useArgPath": true
            }
        },
       
      "DataClumpDetection": {
            "handler": "DataClumpDetectorStep",
            "args": {
                "metricCombiner": "MetricCombiner",
                "languageModel": "AbstractLanguageModel",
                "ranker": "RandomRanker"
            }
        },
       
 
       
        "DataClumpFiltering": {
            "handler": "DataClumpFilterStepHandler",
            "args":{
                "filterName":"ByPathFilter",
                "strictSize":true,
                "handlers":[
                    "SystemInstructionHandler"
                ]
            }
        },
        
        "Refactoring": {
            "handler": "DoNothingStepHandler",
            "args": {
                "handlers": [
                    "introductionHandler",
                    "refactorInstructionHandler",
                    "AllFilesHandler",
                    "SendHandler"
                ]
            }
        }
    }
}