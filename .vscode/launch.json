    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["config_eval_refactor.json", "src/eval/urls.txt" ],
            "preLaunchTask": "tsc: build - tsconfig.json",
            "program": "${workspaceFolder}/src/playground.ts",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ]
        }
    ]
}